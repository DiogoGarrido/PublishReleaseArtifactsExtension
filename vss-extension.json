{
   "manifestVersion": 1,
   "id": "ReleaseArtifacts",
   "name": "Publish and Download Artifacts from Release Pipelines",
   "version": "1.0.13",
   "publisher": "benjhuser",
   "targets": [
      {
         "id": "Microsoft.VisualStudio.Services"
      }
   ],
   "description": "Tasks for uploading and downloading artifacts in a release pipeline",
   "categories": [
      "Azure Pipelines"
   ],
   "icons": {
      "default": "images/upload_icon.png"
   },
   "content":{
      "details":{
			"path": "overview.md"
		},
		"license": {
			"path": "LICENSE"
		}
   },
   "files": [
      {
         "path": "PublishReleaseArtifactTask"
      },
      {
         "path": "DownloadReleaseArtifactTask"
      }
   ],
   "contributions": [
      {
         "id": "PublishReleaseArtifact",
         "type": "ms.vss-distributed-task.task",
         "targets": [
            "ms.vss-distributed-task.tasks"
         ],
         "properties": {
            "name": "PublishReleaseArtifactTask",
            "icons": "images/upload_icon.png"
         }
      },
      {
         "id": "DownloadReleaseArtifact",
         "type": "ms.vss-distributed-task.task",
         "targets": [
            "ms.vss-distributed-task.tasks"
         ],
         "properties": {
            "name": "DownloadReleaseArtifactTask",
            "icons": "images/download_icon.png"
         }
      }
   ]
}